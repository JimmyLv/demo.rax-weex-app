{
  "name": "rax-demo-app",
  "author": "JimmyLv",
  "version": "0.0.0",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --no-info",
    "build": "rm -rf build && NODE_ENV=production ./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx ./src",
    "lint:fix": "./node_modules/.bin/eslint --ext .js --ext .jsx ./src --fix"
  },
  "dependencies": {
    "rax": "^0.2.5",
    "rax-text": "^0.2.5",
    "rax-view": "^0.2.5"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-rax": "^0.2.5",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "html-webpack-plugin": "^2.24.1",
    "image-source-loader": "^0.1.1",
    "internal-ip": "^1.2.0",
    "json-loader": "^0.5.4",
    "qrcode-terminal": "^0.11.0",
    "rax-webpack-plugin": "^0.2.5",
    "stylesheet-loader": "^0.2.5",
    "watch-missing-node-modules-webpack-plugin": "0.0.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "semi": [2, "never"],
      "no-unused-vars": ["error", { "varsIgnorePattern": "createElement" }],
      "react/react-in-jsx-scope": 0,
      "react/jsx-filename-extension": 0
    }
  }
}
